            <key code="101" output="8"/>
            <!-- F9 -> 8 -->
            <key code="109" output="9"/>
            <!-- F10 -> 9 -->
            <key code="103" output="0"/>
            <!-- F11 -> 0 -->
            <key code="111" output="\"/>
            <!-- F12 -> \ (backslash) -->
            
            <!-- Navigation Cluster Symbol Mappings -->
            <key code="105" output="-"/>
            <!-- Print Screen -> - (hyphen) -->
            <key code="107" output="="/>
            <!-- Scroll Lock -> = (equal) -->
            <key code="113" output="["/>
            <!-- Pause -> [ (open bracket) -->
            <key code="114" output=";"/>
            <!-- Insert -> ; (semicolon) -->
            <key code="115" output="'"/>
            <!-- Home -> ' (quote) -->
            <key code="116" output="]"/>
            <!-- Page Up -> ] (close bracket) -->
            <key code="117" output=","/>
            <!-- Delete Forward -> , (comma) -->
            <key code="119" output="."/>
            <!-- End -> . (period) -->
            <key code="121" output="/"/>
            <!-- Page Down -> / (slash) -->
            
